{
  "name": "mm-gas",
  "version": "1.0.0",
  "description": "An application for managing couples' money on LINE.",
  "private": true,
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/genki-sano/gas-ts-template.git"
  },
  "author": "Genki Sano (https://github.com/genki-sano)",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "deploy": "yarn build && yarn clasp push",
    "lint-staged": "lint-staged",
    "eslint": "eslint --ext .js,.ts 'src/**/*'",
    "prettier": "prettier 'src/**/*'",
    "lint:eslint": "yarn eslint --no-error-on-unmatched-pattern",
    "lint:prettier": "yarn prettier --check --no-error-on-unmatched-pattern",
    "fix:eslint": "yarn eslint --fix",
    "fix:prettier": "yarn prettier --write"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.7",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@google/clasp": "^2.4.2",
    "@types/google-apps-script": "^1.0.66",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "babel-loader": "^9.1.3",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "gas-webpack-plugin": "^2.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "volta": {
    "node": "18.17.0",
    "yarn": "1.22.19"
  },
  "lint-staged": {
    "*.ts": [
      "yarn fix:eslint",
      "yarn fix:prettier"
    ]
  }
}
